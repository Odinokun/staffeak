extends ../_template

block variables
  - let title       = 'Staffeak'
  - let description = 'Staffeak'

block content
  #main-wrap.main-wrap
    include ../sections/header-bo-ava

    //begin content
    main.main
      //BEGIN page-top
      .page-top.bg-rounds.search-no-res-page-top
        .container
          .search-no-res-page-top__title-wrap
            a(href="#").search-no-res-page-top__back-btn
              +svg('arr-right')
            h2.page-top__title.search-no-res-page-top__title Result
          .search-no-res-page-top__descr We are finded <span>1,208</span> temp staff for your company
      //END   page-top

      //BEGIN search-filter-sec
      .search-filter-sec
        .container
          .search-filter-sec-mob
            .search-filter-btn.popup-open(data-popup="filter-popup")
              span Filter
              +svg('filter')
          .search-filter-sec-desk
            include ../elements/search-filter
      //END   search-filter-sec


      //BEGIN sponsors
      .sponsors
        .container
          #sponsors__slider.sponsors__slider
            .swiper-wrapper
              .swiper-slide.sponsors__slide
                include ../elements/p-cards/p-card-01
                include ../elements/p-cards/p-card-02
                include ../elements/p-cards/p-card-03
                include ../elements/p-cards/p-card-04
                include ../elements/p-cards/p-card-05
              .swiper-slide.sponsors__slide
                include ../elements/p-cards/p-card-04
                include ../elements/p-cards/p-card-02
                include ../elements/p-cards/p-card-05
                include ../elements/p-cards/p-card-03
                include ../elements/p-cards/p-card-01
              .swiper-slide.sponsors__slide
                include ../elements/p-cards/p-card-05
                include ../elements/p-cards/p-card-02
                include ../elements/p-cards/p-card-03
                include ../elements/p-cards/p-card-01
            .swiper-pagination
      //END   sponsors
    //end content

    include ../sections/footer
    include ../elements/popups
    include ../sections/scripts
